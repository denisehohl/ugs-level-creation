				float time12 = 0.0;
				float2 voronoiSmoothId12 = 0;
				float2 coords12 = IN.ase_texcoord9.xyz.xy * _IceNoiseScale;
				float2 id12 = 0;
				float2 uv12 = 0;
				float fade12 = 0.5;
				float voroi12 = 0;
				float rest12 = 0;
				for( int it12 = 0; it12 <3; it12++ ){
				voroi12 += fade12 * voronoi12( coords12, time12, id12, uv12, 0,voronoiSmoothId12 );
				rest12 += fade12;
				coords12 *= 2;
				fade12 *= 0.5;
