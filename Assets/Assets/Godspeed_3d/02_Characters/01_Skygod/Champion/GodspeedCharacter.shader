				voroi12 /= rest12;
				float FreezeState74 = _Freeze;
				float FreezeAlpha84 = ( step( voroi12 , FreezeState74 ) * ( ( 1.0 - WorldNormal.y ) * FreezeState74 ) );
				float4 lerpResult10 = lerp( lerpResult93 , lerpResult14 , FreezeAlpha84);
				float2 texCoord71 = IN.ase_texcoord8.xy * float2( 1,1 ) + float2( 0,0 );
				float simplePerlin2D50 = snoise( texCoord71*_GooNoiseSize );
				simplePerlin2D50 = simplePerlin2D50*0.5 + 0.5;
				float smoothstepResult63 = smoothstep( 0.35 , 0.5 , simplePerlin2D50);
				float4 GooColor57 = ( smoothstepResult63 * _GooColor );
				float GooMask55 = ( smoothstepResult63 * _Goo );
				float4 lerpResult58 = lerp( lerpResult10 , GooColor57 , GooMask55);
				float4 Albedo45 = lerpResult58;
				
				float2 uv_Normal = IN.ase_texcoord8.xy * _Normal_ST.xy + _Normal_ST.zw;
				float2 uv_IceNormals = IN.ase_texcoord8.xy * _IceNormals_ST.xy + _IceNormals_ST.zw;
				float3 unpack30 = UnpackNormalScale( tex2D( _IceNormals, uv_IceNormals ), 3.0 );
				unpack30.z = lerp( 1, unpack30.z, saturate(3.0) );
				float3 lerpResult29 = lerp( UnpackNormalScale( tex2D( _Normal, uv_Normal ), 1.0f ) , unpack30 , FreezeAlpha84);
				float3 Normals82 = lerpResult29;
				
				float fresnelNdotV42 = dot( WorldNormal, WorldViewDirection );
				float fresnelNode42 = ( 0.0 + 0.54 * pow( 1.0 - fresnelNdotV42, 2.02 ) );
				float2 uv_Emissive = IN.ase_texcoord8.xy * _Emissive_ST.xy + _Emissive_ST.zw;
				float4 lerpResult88 = lerp( ( ( lerpResult10 * fresnelNode42 ) + ( tex2DNode6 * _EmissiveValue ) ) , _EmissiveColor , tex2D( _Emissive, uv_Emissive ).r);
