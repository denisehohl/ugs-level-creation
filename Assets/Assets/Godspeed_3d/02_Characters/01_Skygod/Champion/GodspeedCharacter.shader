				float4 color95 = IsGammaSpace() ? float4(0,0,0,1) : float4(0,0,0,1);
				float Oil98 = _Oil;
				float4 lerpResult93 = lerp( tex2DNode6 , color95 , Oil98);
				float simplePerlin2D25 = snoise( IN.ase_texcoord9.xyz.xy*3.56 );
				simplePerlin2D25 = simplePerlin2D25*0.5 + 0.5;
