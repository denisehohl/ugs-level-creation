				float fresnelNode17 = ( 0.0 + 2.0 * pow( 1.0 - fresnelNdotV17, 1.0 ) );
				float4 Fresnel20 = ( fresnelNode17 * _IceHighlight );
				float4 lerpResult22 = lerp( lerpResult88 , Fresnel20 , FreezeAlpha84);
				float4 lerpResult64 = lerp( lerpResult22 , GooColor57 , GooMask55);
				
